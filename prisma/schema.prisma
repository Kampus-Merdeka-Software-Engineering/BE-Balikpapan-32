// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Pendaftaran {
  id              Int     @id @default(autoincrement())
  nama            String
  email           String
  jenis_konseling String
  pilihan_dokter  String
}

model Feedback {
  id    Int     @id @default(autoincrement())
  nama  String
  pesan String
}

model OurTeam {
  id    Int @id @default(autoincrement())
  role  String
  nama  String
  prodi String
  Univ  String
  image String
}

model Artikel {
  id            Int @id @default(autoincrement())
  judul         String
  deskripsi     String
  image         String
  link_artikel  String
}

model Vidio {
  id            Int @id @default(autoincrement())
  judul         String
  deskripsi     String
  image         String
  link_vidio  String
}